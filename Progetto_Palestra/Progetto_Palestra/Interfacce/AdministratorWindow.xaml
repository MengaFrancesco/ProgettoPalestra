                        <StackPanel Orientation="Horizontal" Margin="10 0">
                            <Image Height="35" Width="35" Source="/Progetto_Palestra;component/Immagini/qr-code.png"/>
                            <TextBlock Margin="10" Foreground="White"><Run Text="Modifiche Controllor"/><Run Text="i"/></TextBlock>
                        </StackPanel>
                    </ListViewItem>
                    <ListViewItem x:Name="LWAttrezzi" Height="45" Padding="0" BorderBrush="#FF7D7C83" MouseLeftButtonUp="LWAttrezzi_MouseLeftButtonUp">
